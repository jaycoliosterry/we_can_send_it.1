<%- model_class = Parcel -%>
<div class="page-header">
  <h1><%=t '.title', :default => Parcel %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:customer_shipment_cost_pre_tax) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.customer_shipment_cost_pre_tax %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:taxable) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.taxable %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:tax_rate) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.tax_rate %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:tax) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.tax %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:customer_shipment_cost_with_tax) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.customer_shipment_cost_with_tax %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:parcel_items_and_quantity) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.parcel_items_and_quantity %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:weight_of_parcel) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.weight_of_parcel %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:customs_short_declaration) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.customs_short_declaration %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:customs_long_description) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.customs_long_description %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:parcel_height) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.parcel_height %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:parcel_length) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.parcel_length %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:parcel_width) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.parcel_width %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:parcel_number_in_order) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.parcel_number_in_order %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:internal_cost_of_parcel) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.internal_cost_of_parcel %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:shipping_carrier) %></dt>
      <dd><pre class="prettyprint"><%= @parcel.shipping_carrier %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              parcels_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_parcel_path(@parcel), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              parcel_path(@parcel),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

